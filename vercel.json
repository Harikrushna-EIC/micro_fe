{
    "routes": [
        {
            "src": "/mf-manifest.json",
            "headers": {
                "Content-Type": "application/json"
            }
        },
        {
            "src": "/remoteEntry.js",
            "headers": {
                "Content-Type": "application/javascript"
            }
        },
        {
            "src": "/assets/.*\\.css",
            "headers": {
                "Content-Type": "text/css"
            }
        },
        {
            "src": "/assets/.*\\.js",
            "headers": {
                "Content-Type": "application/javascript"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}