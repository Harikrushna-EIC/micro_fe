{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/assets/$1"
        },
        {
            "src": "/mf-manifest.json",
            "dest": "/mf-manifest.json"
        }
    ]
}